{
  "name": "PHP Tabelline",
  "dockerFile": "Dockerfile",
  "settings": {
    "terminal.integrated.shell.linux": "/bin/bash"
  },
  "extensions": [
    "bmewburn.vscode-intelephense-client",
    "esbenp.prettier-vscode",
    "oderwat.indent-rainbow"
  ],
  "forwardPorts": [8000],
  "postCreateCommand": "php -S 0.0.0.0:8000 -t public",
  "remoteUser": "vscode"
}
